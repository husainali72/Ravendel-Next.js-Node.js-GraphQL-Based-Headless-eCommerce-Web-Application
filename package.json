{
  "name": "ravendel",
  "version": "1.0.0",
  "description": "An eCommerce Fortress",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client",
    "start": "nodemon server",
    "server": "export NODE_ENV=production && export PORT=80 && node server &",
    "client": "npm start --prefix client",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "localbuild": "npm install && npm install --prefix client && npm install --prefix frontend && npm run build --prefix client && npm run build --prefix frontend && npm run server",
    "postbuild": "npm install && NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm install --prefix frontend && npm run build --prefix client && npm run build --prefix frontend && npm run server"
  },
  "author": "Husain Bandookwala",
  "license": "MIT",
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.44",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.10.1",
    "apollo-upload-client": "^12.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "formidable": "^1.2.1",
    "fs": "0.0.1-security",
    "graphql": "^14.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.3",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "install": "^0.13.0",
    "jimp": "^0.9.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.1",
    "node-pre-gyp": "^0.14.0",
    "nodemon": "^2.0.2",
    "sanitize-html": "^1.21.1",
    "slugify": "^1.3.6",
    "validator": "^12.2.0",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.11"
  }
}
